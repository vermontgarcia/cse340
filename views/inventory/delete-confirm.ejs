<% if (title) { %>
<h1><%= title %></h1>
<% } else { res.redirect('/') } %> <% messages() %>

<form id="delete-form" action="/inv/delete/<%- invId %>" method="post">
  <p>
    Are you sure you want to delete this vehicle? Delete is permanent and cannot
    be undone
  </p>
  <div class="vehicle-card"><%- grid %></div>
  <fieldset class="submit-btn-container">
    <a class="cancel-btn" href="/inv">Cancel</a>
    <input
      id="submit-btn"
      class="delete"
      type="submit"
      role="button"
      value="Delete"
    />
  </fieldset>
</form>
